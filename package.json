{
  "name": "egg-watcher",
  "version": "0.0.2",
  "description": "file watcher plugin for egg",
  "eggPlugin": {
    "name": "watcher"
  },
  "files": [
    "app.js",
    "agent.js",
    "config",
    "lib"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint --ext js . --fix",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "dependencies": {
    "sdk-base": "~1.1.0",
    "wt": "~1.1.0"
  },
  "devDependencies": {
    "autod": "2",
    "egg": "*",
    "egg-bin": "1",
    "egg-ci": "1",
    "egg-mock": "*",
    "eslint": "3",
    "eslint-config-egg": "3",
    "should": "^9.0.2",
    "supertest": "^1.2.0"
  },
  "homepage": "https://github.com/eggjs/egg-watcher",
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/egg-watcher.git"
  },
  "keywords": [
    "egg-watcher",
    "egg",
    "watcher",
    "watch"
  ],
  "engines": {
    "node": ">= 4.0.0"
  },
  "ci": {
    "version": "4, 6"
  }
